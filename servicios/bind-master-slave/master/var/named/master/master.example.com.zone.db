$TTL 100
$ORIGIN example.com.
@	IN SOA	dns.example.com. hostmaster.example.com. (
					2018122902 ;	 serial
					1D 	 ; refresh
					1H 	 ; retry
					1W  	 ; expire
					3H )	 ; minimum
	IN	NS	dns.example.com.
	IN 	MX	10	correo.example.com.
dns	IN	A	192.168.122.3
provider	IN	CNAME dns.example.com.
correo	IN	A	192.168.122.4
ldap	IN	A	192.168.122.199
desktop	IN	A	192.168.122.120
cliente IN	CNAME	desktop.example.com.
server	IN	A	192.168.122.121
servidor IN	CNAME	server.example.com.
intruder	IN	A	192.168.122.150
intruso	IN	CNAME	intruder.example.com.
guest  IN      A       192.168.122.123
invitado  IN      A    guest.example.com.

$ORIGIN idm.example.com.
@             IN        NS      ipa.idm.example.com.
              IN        NS      dns.example.com.
